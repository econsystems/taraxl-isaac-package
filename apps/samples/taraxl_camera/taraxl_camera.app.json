      {
        "name": "taraxl",
        "components": [
          {
            "name": "message_ledger",
            "type": "isaac::alice::MessageLedger"
          },
          {
            "name": "isaac.TaraXLCameraDevice",
            "type": "isaac::TaraXLCameraDevice"
          }
        ]
      }
    ]
